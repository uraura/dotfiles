	name = uraura
[alias]
	cut-away = !git branch --merged origin/master | \\grep -v '*' | xargs git branch -d
	ch = !git branch -a | fzf-tmux -d 30% | xargs git checkout
	co = commit
	fe = fetch -p
	root = rev-parse --show-toplevel
[core]
	excludesfile = /Users/uraura/.config/git/.gitignore_global
[filter "lfs"]
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
	clean = git-lfs clean -- %f
[ghq]
	root = ~/
